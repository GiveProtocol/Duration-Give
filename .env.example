# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to git!

# Server Configuration
PORT=8000
NODE_ENV=development

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Application Domain
VITE_APP_DOMAIN=http://localhost:5173

# Blockchain Configuration
VITE_MOONBASE_RPC_URL=https://rpc.api.moonbase.moonbeam.network
VITE_MOONBEAM_MAINNET_RPC_URL=https://rpc.api.moonbeam.network

# Hardhat Private Keys (Development Only - Use test accounts)
PRIVATE_KEY=your_test_private_key_here
PRIVATE_KEY_2=your_second_test_private_key_here
PRIVATE_KEY_3=your_third_test_private_key_here

# External Services
VITE_SENTRY_DSN=your_sentry_dsn_here

# Development Configuration
VITE_DEVELOPMENT_MODE=true
VITE_DEBUG_MODE=false

# Docker/Database Configuration (Development)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_strong_postgres_password_here
POSTGRES_DB=give_protocol_dev

# Security Note: 
# - Use strong, unique credentials for production
# - Rotate keys regularly
# - Never commit .env files to version control
# - Use environment-specific configurations
# - Generate strong passwords for all services